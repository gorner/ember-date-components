{
  "name": "ember-date-components",
  "version": "4.0.1",
  "description": "An Ember add-on which provides pure Ember-based date picker components.",
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"yarn:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"yarn:lint:css -- --fix\"",
    "lint:fix": "concurrently \"yarn:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint ./addon ./app",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "run-prettier": "prettier --write \"**/*.{js,scss}\"",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "concurrently \"yarn:lint\" \"yarn:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "test:one": "ember try:one"
  },
  "peerDependencies": {
    "ember-moment": ">=9.0.0",
    "ember-source": "^4.0.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/eslint-parser": "^7.25.1",
    "@babel/helper-get-function-arity": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.24.7",
    "@ember/optional-features": "~2.1.0",
    "@ember/test-helpers": "^3.3.1",
    "@embroider/test-setup": "^4.0.0",
    "bootstrap": "^4.3.1",
    "broccoli-asset-rev": "~3.0.0",
    "concurrently": "^8.2.2",
    "ember-auto-import": "~2.8.1",
    "ember-bootstrap": "~5.1.1",
    "ember-cli": "~5.12.0",
    "ember-cli-app-version": "~5.0.0",
    "ember-cli-clean-css": "^3.0.0",
    "ember-cli-dependency-checker": "~3.3.2",
    "ember-cli-github-pages": "~0.2.0",
    "ember-cli-inject-live-reload": "~2.1.0",
    "ember-cli-release-tag": "~0.1.5",
    "ember-cli-sass": "~10.0.1",
    "ember-cli-terser": "^4.0.2",
    "ember-focus-trap": "^1.0.1",
    "ember-load-initializers": "~2.1.2",
    "ember-moment": "~10.0.0",
    "ember-page-title": "^8.2.3",
    "ember-prism": "~0.13.0",
    "ember-qunit": "~8.1.0",
    "ember-resolver": "~12.0.1",
    "ember-source": "~5.12.0",
    "ember-source-channel-url": "~3.0.0",
    "ember-template-lint": "~6.0.0",
    "ember-try": "~3.0.0",
    "eslint": "~8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "~12.2.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-qunit": "^8.1.2",
    "loader.js": "~4.7.0",
    "moment": "^2.29.4",
    "prettier": "^3.3.3",
    "qunit": "^2.22.0",
    "qunit-dom": "~3.2.1",
    "sass": "~1.32.11",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-prettier": "^4.1.0",
    "webpack": "^5.95.0"
  },
  "dependencies": {
    "@babel/core": "^7.25.2",
    "@ember/render-modifiers": "^2.0.4",
    "@glimmer/component": "~1.1.2",
    "@glimmer/tracking": "~1.1.2",
    "ember-basic-dropdown": "^6.0.0",
    "ember-cached-decorator-polyfill": "^1.0.1",
    "ember-classic-decorator": "^3.0.0",
    "ember-cli-babel": "^8.2.0",
