  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://mydea.github.io/ember-date-components/"
  },
  "volta": {
    "node": "12.16.3",
    "yarn": "1.22.4"
  },
